    <triggers>
        <trigger>
            <expression>{AntMiner S17:AntMinerL3.Hashrate.nodata(15m)}=1 and {AntMiner S17:icmpping.last()}=1</expression>
            <name>AntMiner S17 Hashrate information not available on {HOST.NAME}</name>
            <url>http://{HOST.CONN}</url>
            <priority>HIGH</priority>
            <manual_close>YES</manual_close>
        </trigger>
        <trigger>
            <expression>(({AntMiner S17:AntMinerL3.Temp1.min(15m)}&gt;{$TEMP_LOW} or {AntMiner S17:AntMinerL3.Temp2.min(15m)}&gt;{$TEMP_LOW} or {AntMiner S17:AntMinerL3.Temp3.min(15m)}&gt;{$TEMP_LOW} or {AntMiner S17:AntMinerL3.Temp4.min(15m)}&gt;{$TEMP_LOW}) and {AntMiner S17:icmpping.last()}=1)</expression>
            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
            <recovery_expression>(({AntMiner S17:AntMinerL3.Temp1.last()}&lt;{$TEMP_LOW} or {AntMiner S17:AntMinerL3.Temp2.last()}&lt;{$TEMP_LOW} or {AntMiner S17:AntMinerL3.Temp3.last()}&lt;{$TEMP_LOW} or {AntMiner S17:AntMinerL3.Temp4.last()}&lt;{$TEMP_LOW}) and {AntMiner S17:icmpping.last()}=1)</recovery_expression>
            <name>AntMiner S17 High temp on {HOST.NAME}</name>
            <url>http://{HOST.CONN}</url>
            <priority>AVERAGE</priority>
            <manual_close>YES</manual_close>
        </trigger>
        <trigger>
            <expression>{AntMiner S17:icmpping.max(#3)}=0 and {AntMiner S17:net.tcp.service[tcp,,4028].last(#3)}=0</expression>
            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
            <recovery_expression>{AntMiner S17:icmpping.last()}=1 or {AntMiner S17:net.tcp.service[tcp,,4028].last()}=1</recovery_expression>
            <name>AntMiner S17 Host DOWN {HOST.NAME}</name>
            <url>http://{HOST.CONN}</url>
            <priority>DISASTER</priority>
            <manual_close>YES</manual_close>
        </trigger>
        <trigger>
            <expression>({AntMiner S17:AntMinerL3.Hashrate.last(240)}&lt;{$HASH_LOW} and {AntMiner S17:AntMinerL3.Elapsed.last()}&gt;600) and ({AntMiner S17:icmpping.last()}=1)</expression>
            <name>AntMiner S17 Low hashrate on {HOST.NAME}, {ITEM.LASTVALUE}</name>
            <url>http://{HOST.CONN}</url>
            <priority>WARNING</priority>
            <manual_close>YES</manual_close>
        </trigger>
        <trigger>
            <expression>({AntMiner S17:AntMinerL3.Fan1.last()}=0 or {AntMiner S17:AntMinerL3.Fan2.last()}=0) and ({AntMiner S17:icmpping.last()}=1)</expression>
            <name>AntMiner S17 пропеллер сломан {HOST.NAME}</name>
            <url>http://{HOST.CONN}</url>
            <manual_close>YES</manual_close>
        </trigger>
        <trigger>
            <expression>{AntMiner S17:AntMinerL3.Hashrate.last(240)}&lt;{$HASH_LOW} and {AntMiner S17:icmpping.last()}=1</expression>
            <name>new AntMiner S17 Low hashrate on {HOST.NAME}</name>
            <url>http://{HOST.CONN}</url>
            <status>DISABLED</status>
            <priority>WARNING</priority>
            <manual_close>YES</manual_close>
        </trigger>
        <trigger>
            <expression>{AntMiner S17:AntMinerL3.Fan_num.last()}=0 and {AntMiner S17:icmpping.last()}=1 and ({AntMiner S17:AntMinerL3.Elapsed.last()} = {AntMiner S17:AntMinerL3.Elapsed.prev()} and {AntMiner S17:AntMinerL3.Elapsed.last()}&gt;0)</expression>
            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
            <recovery_expression>{AntMiner S17:AntMinerL3.Fan_num.last()}=2</recovery_expression>
            <name>Асик завис</name>
            <manual_close>YES</manual_close>
        </trigger>
        <trigger>
            <expression>({AntMiner S17:AntMinerL3.URL1.str({$URL1})}=0 or {AntMiner S17:AntMinerL3.URL2.str({$URL2})}=0 or {AntMiner S17:AntMinerL3.URL3.str({$URL3})}=0) and {AntMiner S17:icmpping.last()}=1</expression>
            <name>Внимание &quot;левые&quot; URL! {HOST.NAME}</name>
            <status>DISABLED</status>
            <manual_close>YES</manual_close>
        </trigger>
        <trigger>
            <expression>{AntMiner S17:AntMinerL3.Userpool3.strlen()}&lt;&gt;0 or {AntMiner S17:AntMinerL3.Userpool4.strlen()}&lt;&gt;0 and {AntMiner S17:icmpping.last()}=1</expression>
